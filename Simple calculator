print("please select operation")
print("1,add")
print("2,sub")
print("3,mul")
print("4,div")
choice = int(input("enter your choice:"))
n1 = int(input("number1:"))
n2 = int( input("number2:"))
if choice == 1:
print(n1+n2)
elif choice == 2:
print(n1-n2)
elif choice == 3:
print(n1*n2)
elif choice == 4:
print(n1/n2)
else:
print("enter correct choice")
